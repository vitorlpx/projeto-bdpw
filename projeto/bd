CREATE TABLE `bdpw3`.`demo2` (`idContato` INT NOT NULL AUTO_INCREMENT , `name` VARCHAR(40) NOT NULL , `email` VARCHAR(40) NOT NULL , `cpf` VARCHAR(40) NOT NULL , `rg` VARCHAR(40) NOT NULL , `telefone` VARCHAR(15) NOT NULL , `estado` VARCHAR(40) NOT NULL , `cidade` VARCHAR(40) NOT NULL , `nomemae` VARCHAR(40) NOT NULL , `estadocivil` VARCHAR(40) NOT NULL , `ocupacao` VARCHAR(40) NOT NULL , PRIMARY KEY (`idContato`)) ENGINE = InnoDB;